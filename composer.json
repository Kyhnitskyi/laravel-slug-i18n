{
    "name": "kyhnitskyi/laravel-slug-i18n",
    "description": "A Laravel package to generate multilingual slugs (UTF-8) with optional transliteration skipping for specific locales.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Eslam El Sherif",
            "email": "dev@eslamelsherif.com"
        }
    ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^8.0|^9.0"
  },
    "autoload": {
        "psr-4": {
            "Developvi\\LaravelSlugI18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Developvi\\LaravelSlugI18n\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Developvi\\LaravelSlugI18n\\SlugI18nServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
    
}
